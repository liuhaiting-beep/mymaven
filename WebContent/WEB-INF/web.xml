<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>myWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StudentAction</servlet-name>
    <servlet-class>com.accp.t1.action.StudentAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentAction</servlet-name>
    <url-pattern>/c/student</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TenderAction</display-name>
    <servlet-name>TenderAction</servlet-name>
    <servlet-class>com.accp.t2.action.TenderAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TenderAction</servlet-name>
    <url-pattern>/c/tender</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MembersinfoAction</display-name>
    <servlet-name>MembersinfoAction</servlet-name>
    <servlet-class>com.accp.t3.action.MembersinfoAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MembersinfoAction</servlet-name>
    <url-pattern>/c/member</url-pattern>
  </servlet-mapping>
</web-app>